<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.studentsdatebase">

    <application
        android:allowBackup="true"
        android:icon="@drawable/unknown"
        android:label="@string/app_name"
        android:roundIcon="@drawable/unknown"
        android:supportsRtl="true"
        android:theme="@style/Theme.StudentsDateBase">
        <activity
            android:name=".ChooseStudentActivity"
            android:exported="false" />
        <activity
            android:name=".AddStudentActivity"
            android:exported="false" />
        <activity
            android:name=".ChooseGroupActivity"
            android:exported="false" />
        <activity
            android:name=".AddGroupActivity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>